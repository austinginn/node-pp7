{
    "source": {
      "include": ["src"],
      "includePattern": ".mjs$",
      "excludePattern": "(node_modules/|docs)"
    },
    "plugins": ["plugins/markdown"],
    "templates": {
      "cleverLinks": true,
      "monospaceLinks": true
    },
    "opts": {
      "recurse": true,
      "destination": "./jsdoc/docs/",
      "template": "./jsdoc//custom-template",
      "tutorials": "./jsdoc/tutorials",
      "readme": "./jsdoc/readme/readme.md"
    }
  }