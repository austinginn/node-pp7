{
    "source": {
      "include": ["src"],
      "includePattern": ".mjs$",
      "excludePattern": "(node_modules/|docs)"
    },
    "plugins": ["plugins/markdown"],
    "templates": {
      "cleverLinks": true,
      "monospaceLinks": true
    },
    "opts": {
      "recurse": true,
      "destination": "./docs/jsdocs/",
      "template": "./docs/custom-template",
      "tutorials": "./docs/tutorials",
      "readme": "./docs/readme/readme.md"
    }
  }